{
  "name": "@pinata/grapevine-sdk",
  "version": "0.1.4",
  "description": "Easy-to-use SDK for the Grapevine API - Create and manage content feeds with x402 micropayments",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "grapevine": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:cli": "bun build ./src/cli/index.ts --compile --minify --sourcemap --bytecode --outfile grapevine-cli",
    "build:cli:all": "bun run build:cli:macos-x64 && bun run build:cli:macos-arm && bun run build:cli:linux && bun run build:cli:windows",
    "build:cli:macos-x64": "bun build ./src/cli/index.ts --compile --target=bun-darwin-x64 --minify --sourcemap --bytecode --outfile dist/bin/grapevine-macos-x64",
    "build:cli:macos-arm": "bun build ./src/cli/index.ts --compile --target=bun-darwin-arm64 --minify --sourcemap --bytecode --outfile dist/bin/grapevine-macos-arm64",
    "build:cli:linux": "bun build ./src/cli/index.ts --compile --target=bun-linux-x64 --minify --sourcemap --bytecode --outfile dist/bin/grapevine-linux",
    "build:cli:windows": "bun build ./src/cli/index.ts --compile --target=bun-windows-x64 --minify --sourcemap --bytecode --outfile dist/bin/grapevine-windows.exe",
    "build:all": "bun run build && bun run build:cli",
    "dev": "tsc --watch",
    "test": "bun test",
    "test:node": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest",
    "test:cli": "bun run build:cli && bun run test:cli:exec",
    "test:cli:exec": "./grapevine-cli --version && ./grapevine-cli --help && ./grapevine-cli info",
    "test:all": "bun run test:bun && bun run test:cli",
    "test:bun": "bun test",
    "test:integration": "bun test tests/integration/",
    "test:e2e": "bun test tests/e2e/ --timeout 60000 --verbose",
    "test:e2e:sdk": "bun test tests/e2e/sdk-e2e.test.ts --timeout 60000 --verbose",
    "test:e2e:cli": "bun test tests/e2e/cli-e2e.test.ts --timeout 60000 --verbose",
    "example": "bun run examples/basic-usage.ts",
    "prepublishOnly": "bun run build:all",
    "test:manual": "cd tests/manual && bun install && bun run dev",
    "test:manual:build": "cd tests/manual && bun run build"
  },
  "keywords": [
    "grapevine",
    "api",
    "sdk",
    "x402",
    "micropayments",
    "content",
    "feeds",
    "base",
    "web3"
  ],
  "dependencies": {
    "viem": "^2.39.0",
    "@coinbase/x402": "^0.7.1",
    "x402": "^0.7.1",
    "commander": "^11.0.0",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/mime-types": "^2.1.4",
    "@types/react": "^18.0.0",
    "typescript": "^5.0.0",
    "@types/bun": "latest",
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0",
    "wagmi": ">=2.0.0",
    "@tanstack/react-query": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "wagmi": {
      "optional": true
    },
    "@tanstack/react-query": {
      "optional": true
    }
  },
  "author": "Grapevine Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PinataCloud/grapevine-sdk.git"
  },
  "homepage": "https://github.com/PinataCloud/grapevine-sdk#readme",
  "bugs": {
    "url": "https://github.com/PinataCloud/grapevine-sdk/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "!dist/bin/"
  ]
}