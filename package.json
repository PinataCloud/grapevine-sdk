{
  "name": "@pinata/grapevine-sdk",
  "version": "0.1.9",
  "description": "Easy-to-use SDK for the Grapevine API - Create and manage content feeds with x402 micropayments",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "bun test tests/integration/ --timeout 30000",
    "test:unit": "bun test tests/unit/",
    "test:integration": "bun test tests/integration/ --timeout 30000",
    "test:e2e": "bun test tests/e2e/sdk-e2e.test.ts --timeout 60000 --verbose",
    "test:all": "bun test",
    "example": "bun run examples/basic-usage.ts",
    "prepublishOnly": "bun run build",
    "test:manual": "cd tests/manual && bun install && bun run dev",
    "test:manual:build": "cd tests/manual && bun run build"
  },
  "keywords": [
    "grapevine",
    "api",
    "sdk",
    "x402",
    "micropayments",
    "content",
    "feeds",
    "base",
    "web3"
  ],
  "dependencies": {
    "viem": "^2.39.0",
    "@coinbase/x402": "^0.7.1",
    "x402": "^0.7.1",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/mime-types": "^2.1.4",
    "@types/react": "^18.0.0",
    "typescript": "^5.0.0",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0",
    "wagmi": ">=2.0.0",
    "@tanstack/react-query": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "wagmi": {
      "optional": true
    },
    "@tanstack/react-query": {
      "optional": true
    }
  },
  "author": "Grapevine Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PinataCloud/grapevine-sdk.git"
  },
  "homepage": "https://github.com/PinataCloud/grapevine-sdk#readme",
  "bugs": {
    "url": "https://github.com/PinataCloud/grapevine-sdk/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ]
}
